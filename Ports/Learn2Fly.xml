<?xml version="1.0" encoding="UTF-8" ?>
<Module>
<ModulePrefs title="Funny Shooter 2" />
<Content type="html"><![CDATA[
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>AwayFL Fullscreen Example</title>
    <script src="https://cdn.jsdelivr.net/gh/awayfl/awayfl-embed/dist/embed.js"></script>
    <style>
        html, body {
            margin: 0;
            padding: 0;
            width: 100%;
            height: 100%;
            overflow: hidden;
        }
        #container {
            width: 100%;
            height: 100%;
        }
    </style>
</head>
<body>
    <div id="container"></div>

    <script>
        window.AWAY_EMBED_CFG = {}; 
        // Use 100% width and height to fill the screen
        swfObject.embedSWF('https://jjjwelch420.github.io/Launcher/Ports/SWF/Learn2Fly.swf', 'container', '100%', '100%');
    </script>
</body>
</html>
]]></Content>
</Module>
